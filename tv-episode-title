#!/usr/bin/python3

import re

episode_title = "Show Name S01E01: Episode Title"
pattern = r"^(.+)\sS\d{2}E\d{2}:\s(.+)$"
match = re.match(pattern, episode_title)

if match:
    show_name = match.group(1)
    season_number = match.group(2)[:2]
    episode_number = match.group(2)[3:5]
    episode_title = match.group(3)
    
    # Print the extracted information
    print("Show Name:", show_name)
    print("Season Number:", season_number)
    print("Episode Number:", episode_number)
    print("Episode Title:", episode_title)
else:
    print("Invalid episode title.")  
